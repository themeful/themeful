{
  "root": "libs/property-types",
  "projectType": "library",
  "sourceRoot": "libs/property-types/src",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/property-types/**/*.ts",
          "libs/property-types/**/*.tsx"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/property-types"],
      "options": {
        "jestConfig": "libs/property-types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:properties"]
}

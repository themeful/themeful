{
  "name": "themeful",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "run-p frontend backend",
    "dev:ui": "run-p themeful-ui storybook",
    "themeful-ui": "nx run themeful-ui:build:watch",
    "frontend": "nx run frontend:serve:watch",
    "backend": "nx run backend:serve",
    "storybook": "nx run themeful-ui:storybook",
    "nx": "nx",
    "serve": "nx serve",
    "build": "nx build",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest && yarn && if [ -f 'migrations.json' ]; then nx migrate --run-migrations=migrations.json && yarn install; fi",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "build:themeful-ui": "nx run themeful-ui:build",
    "watch:themeful-ui": "nx run themeful-ui:build:watch",
    "lint": "nx lint",
    "test": "nx test",
    "lint:all": "nx workspace-lint && nx run-many --target=lint --all",
    "test:all": "nx run-many --target=test --all",
    "build:all": "nx run themeful-ui:build && nx run frontend:build && nx run backend:build"
  },
  "private": true,
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",

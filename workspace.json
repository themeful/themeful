{
  "version": 2,
  "projects": {
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["dist/apps/backend"],
          "options": {
            "outputPath": "dist/apps/backend",
            "main": "apps/backend/src/main.ts",
            "tsConfig": "apps/backend/tsconfig.app.json",
            "assets": ["apps/backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/backend/src/environments/environment.ts",
                  "with": "apps/backend/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backend:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/backend/**/*.ts", "apps/backend/**/*.tsx"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/backend"],
          "options": {
            "jestConfig": "apps/backend/jest.config.js"
          }
        }
      },
      "tags": ["scope:backend"]
    },
    "frontend": {
      "root": "libs/frontend",
      "sourceRoot": "libs/frontend/src",
      "projectType": "library",
      "generators": {
        "@nxext/stencil:component": {
          "style": "scss"
        }
      },
      "targets": {
        "test": {
          "executor": "@nxext/stencil:test",
          "outputs": ["coverage/libs/frontend"],
          "options": {
            "projectType": "library",
            "tsConfig": "libs/frontend/tsconfig.spec.json",
            "configPath": "libs/frontend/stencil.config.ts",
            "outputPath": "dist/libs/frontend"
          }
        },
        "build": {
          "executor": "@nxext/stencil:build",
          "outputs": ["dist/libs/frontend"],
          "options": {
            "projectType": "library",
            "tsConfig": "libs/frontend/tsconfig.lib.json",
            "configPath": "libs/frontend/stencil.config.ts",
            "outputPath": "dist/libs/frontend"
          },
          "configurations": {
            "production": {
              "dev": false,
              "prod": true
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/frontend/**/*.ts", "libs/frontend/**/*.tsx"]
          }
        },
        "serve": {
          "executor": "@nxext/stencil:build",
          "outputs": ["dist/libs/frontend"],
          "options": {
            "projectType": "library",
            "tsConfig": "libs/frontend/tsconfig.lib.json",
            "configPath": "libs/frontend/stencil.config.ts",
            "outputPath": "dist/libs/frontend",
            "serve": true,
            "watch": true,
            "proxyConfig": "libs/frontend/proxy.conf.json"
          }
        },
        "e2e": {
          "executor": "@nxext/stencil:e2e",
          "outputs": ["dist/libs/frontend"],
          "options": {
            "projectType": "library",
            "tsConfig": "libs/frontend/tsconfig.lib.json",
            "configPath": "libs/frontend/stencil.config.ts",
            "outputPath": "dist/libs/frontend"
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/html",
            "port": 4400,
            "config": {
              "configFolder": "libs/frontend/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["dist/storybook/frontend"],
          "options": {
            "uiFramework": "@storybook/html",
            "outputPath": "dist/storybook/frontend",
            "config": {
              "configFolder": "libs/frontend/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": ["scope:frontend"]
    },
    "property-types": {
      "root": "libs/property-types",
      "sourceRoot": "libs/property-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/property-types/**/*.ts", "libs/property-types/**/*.tsx"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/property-types"],
          "options": {
            "jestConfig": "libs/property-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:properties"]
    },
    "typings": {
      "root": "libs/typings",
      "sourceRoot": "libs/typings/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/typings/**/*.ts", "libs/typings/**/*.tsx"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/typings"],
          "options": {
            "jestConfig": "libs/typings/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:typings"]
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts", "libs/utils/**/*.tsx"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["scope:utils"]
    }
  }
}
